info:
  title: devenv service account
  author: Tatsuro Fujii
  description: Create a service account for devenv
  version: 1.0

required:
  - roles
  - googleapi_agent_roles

properties:
  roles:
    type: array
    description: Roles granted to created service account

  googleapi_agent_roles:
    type: array
    description: |
      Roles granted to Google APIs Service Agent.
      Deployment Manager delegates this agent to run actual operations.
